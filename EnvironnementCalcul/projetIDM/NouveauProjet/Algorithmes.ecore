<?xml version="1.0" encoding="UTF-8"?>
<ecore:EPackage xmi:version="2.0" xmlns:xmi="http://www.omg.org/XMI" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:ecore="http://www.eclipse.org/emf/2002/Ecore" name="Algorithmes" nsURI="http://algorithmes" nsPrefix="Algorithmes">
  <eClassifiers xsi:type="ecore:EClass" name="Catalogue">
    <eStructuralFeatures xsi:type="ecore:EReference" name="algo" lowerBound="1" upperBound="-1"
        eType="#//Algorithme" containment="true" eOpposite="#//Algorithme/catalogue"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="Algorithme">
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="nom" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EString"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="catalogue" lowerBound="1"
        eType="#//Catalogue" eOpposite="#//Catalogue/algo"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="elements" upperBound="-1"
        eType="#//AlgoElements" containment="true" eOpposite="#//AlgoElements/algo"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="LaRess" lowerBound="1"
        eType="#//Ressources"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="Ressources">
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="NomFichier" lowerBound="1"
        eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EString"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="Documentation" eSuperTypes="#//AlgoElements">
    <eStructuralFeatures xsi:type="ecore:EReference" name="elements" upperBound="-1"
        eType="#//AlgoElements"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="Entree" eSuperTypes="#//AlgoElements"/>
  <eClassifiers xsi:type="ecore:EClass" name="Sortie" eSuperTypes="#//AlgoElements"/>
  <eClassifiers xsi:type="ecore:EClass" name="AlgoElements">
    <eStructuralFeatures xsi:type="ecore:EReference" name="algo" lowerBound="1" eType="#//Algorithme"
        eOpposite="#//Algorithme/elements"/>
  </eClassifiers>
</ecore:EPackage>
